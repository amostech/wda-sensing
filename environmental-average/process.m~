clear;

[t,x,y,z,mf] = loadData('hour-night.csv');

figure;

subplot(2,2,1);
plot(t,x);
title('X-Axis(uT) vs. Time(s)');

subplot(2,2,2);
plot(t,y);
title('Y-Axis(uT) vs. Time(s)');

subplot(2,2,3);
plot(t,z);
title('Z-Axis(uT) vs. Time(s)');

subplot(2,2,4);
plot(t,mf);
title('X-Axis(uT) vs. Time(s)');
